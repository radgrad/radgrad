{
  "name": "radgrad",
  "private": true,
  "scripts": {
    "start": "meteor --settings ../config/settings.development.json --no-release-check",
    "start-local-production": "meteor --settings ../config/settings.production.local.json --no-release-check",
    "lint": "./node_modules/.bin/eslint --quiet ./imports",
    "lint-loud": "./node_modules/.bin/eslint ./imports",
    "lint-fix": "./node_modules/.bin/eslint --quiet --fix ./imports",
    "pretest": "npm run lint",
    "test": "TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package dispatch:mocha --settings ../config/settings.development.json --no-release-check --port 3100",
    "test-debug": "TEST_BROWSER_DRIVER=nightmare meteor test --once --driver-package dispatch:mocha --no-release-check --port 3100 --inspect-brk=5858",
    "test-app": "METEOR_NO_RELEASE_CHECK=1 TEST_BROWSER_DRIVER=nightmare meteor test --full-app --once --driver-package dispatch:mocha --settings ../config/settings.development.json --port 3100",
    "test-watch": "TEST_WATCH=1 TEST_BROWSER_DRIVER=nightmare meteor test --driver-package dispatch:mocha --no-release-check --port 3100",
    "pretest-win": "npm run lint",
    "test-win": "meteor test --once --driver-package dispatch:mocha --no-release-check --settings ../config/settings.development.json --port 3100",
    "test-app-win": "meteor test --full-app --once --driver-package dispatch:mocha --no-release-check --settings ../config/settings.development.json --port 3100",
    "coverage-cam": "meteor test --driver-package practicalmeteor:mocha --port 3100 --no-release-check --settings ../config/settings.coverage.cam.json",
    "jsdoc": "./node_modules/.bin/jsdoc -c ./jsdoc.json -r .",
    "deploy": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy radgrad.meteorapp.com --settings ../config/settings.production.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^0.8.7",
    "chromedriver": "^2.38.2",
    "jquery": "^3.4.1",
    "jquery-tablesort": "0.0.11",
    "meteor-node-stubs": "^0.2.11",
    "node-balanced": "0.0.14",
    "npm": "^6.10.1",
    "selenium-webdriver": "^3.6.0",
    "simpl-schema": "^0.3.2",
    "webdriverio": "^4.13.2"
  },
  "devDependencies": {
    "chai": "*",
    "docdash": "^0.3.0",
    "electron": "^4.0.5",
    "es6-enum": "^1.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-meteor": "^4.2.1",
    "eslint-plugin-react": "^7.7.0",
    "jsdoc": "^3.6.3",
    "nightmare": "^2.10.0",
    "radgrad-jsdoc-template": "^1.1.3"
  }
}
