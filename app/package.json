{
  "name": "radgrad",
  "private": true,
  "scripts": {
    "start": "meteor --settings ../config/settings.development.json --no-release-check",
    "start-local-production": "meteor --settings ../config/settings.production.local.json --no-release-check",
    "lint": "./node_modules/.bin/eslint --quiet ./imports",
    "lint-loud": "./node_modules/.bin/eslint ./imports",
    "lint-fix": "./node_modules/.bin/eslint --quiet --fix ./imports",
    "pretest": "npm run lint",
    "test": "TEST_BROWSER_DRIVER=puppeteer meteor test --once --driver-package meteortesting:mocha --settings ../config/settings.development.json --no-release-check --port 3100",
    "test-debug": "TEST_BROWSER_DRIVER=puppeteer meteor test --once --driver-package meteortesting:mocha --no-release-check --port 3100 --inspect-brk=5858",
    "test-app": "TEST_BROWSER_DRIVER=puppeteer meteor test --full-app --once --driver-package meteortesting:mocha --settings ../config/settings.development.json --no-release-check --port 3100",
    "test-watch": "TEST_WATCH=1 TEST_BROWSER_DRIVER=puppeteer meteor test --driver-package meteortesting:mocha --no-release-check --port 3100",
    "pretest-win": "npm run lint",
    "test-win": "meteor test --once --driver-package meteortesting:mocha --no-release-check --settings ../config/settings.development.json --port 3100",
    "test-app-win": "meteor test --full-app --once --driver-package meteortesting:mocha --no-release-check --settings ../config/settings.development.json --port 3100",
    "coverage-cam": "meteor test --driver-package practicalmeteor:mocha --port 3100 --no-release-check --settings ../config/settings.coverage.cam.json",
    "jsdoc": "./node_modules/.bin/jsdoc -c ./jsdoc.json -r .",
    "deploy": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy radgrad.meteorapp.com --settings ../config/settings.production.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.6",
    "chromedriver": "^77.0.0",
    "jquery": "^3.4.1",
    "jquery-tablesort": "0.0.11",
    "meteor-node-stubs": "^0.4.1",
    "node-balanced": "0.0.14",
    "npm": "^6.12.0",
    "selenium-webdriver": "^3.6.0",
    "simpl-schema": "^1.5.6",
    "webdriverio": "^5.15.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "docdash": "^1.1.1",
    "electron": "^6.0.12",
    "es6-enum": "^1.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jsdoc": "^3.6.3",
    "nightmare": "^3.0.2",
    "puppeteer": "^1.20.0",
    "radgrad-jsdoc-template": "^1.1.3"
  }
}
